<Window x:Name="build" x:Class="Consultorio.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Consultorio"
        mc:Ignorable="d"
        Title="Login" Height="720" Width="1280" MinHeight="720" MinWidth="1280" WindowStartupLocation="CenterScreen">
    <Window.BindingGroup>
        <BindingGroup/>
    </Window.BindingGroup>
    <Grid>

        <Rectangle Fill="#FFCACAE6" Stroke="Black"/>

        <Rectangle Fill="#FF7FBEE2" Margin="470,102,470,134" Stroke="Black" RadiusY="50.0" RadiusX="50.0"/>

        <Label x:Name="lbLogin" Content="Login" HorizontalAlignment="Left" Margin="510,0,0,296" VerticalAlignment="Bottom" RenderTransformOrigin="0.667,-0.308"/>
        <TextBox x:Name="boxlogin" Margin="557,0,516,296" TextWrapping="Wrap" Height="26" VerticalAlignment="Bottom"/>

        <Label x:Name="lbSenha" Content="Senha" HorizontalAlignment="Left" Margin="510,0,0,250" VerticalAlignment="Bottom"/>
        <PasswordBox x:Name="boxSenha" Margin="557,0,516,255" VerticalAlignment="Bottom" Height="21"/>

        <Button x:Name="btEntrar" Content="Entrar" Margin="606,0,592,205" VerticalAlignment="Bottom" Click="BtEntrar_Click"/>

    </Grid>
</Window>
