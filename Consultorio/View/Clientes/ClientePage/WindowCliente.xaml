<Window x:Class="Consultorio.View.Clientes.ClientePage.WindowCliente"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Consultorio.View.Clientes.ClientePage"
        mc:Ignorable="d"
        Title="WindowCliente" Style="{StaticResource JanelaPersonalizada}" WindowStartupLocation="CenterScreen">
    <Viewbox>
        <Grid Height="720" Width="1324" Margin="0">
            <Frame x:Name="frameCliente" Content="" Margin="0,125,0,70" NavigationUIVisibility="Hidden" />
            <Button x:Name="btGeral" Content="Geral" Margin="-2,93,1252,595" Height="Auto" Click="BtGeral_Click"/>
            <Button x:Name="btAnamnese" Content="Anamnese" Margin="72,93,1149,595" Height="Auto" Click="BtAnamnese_Click"/>
            <Button x:Name="btOdontograma" Content="Odontograma" Margin="175,93,1015,595" Click="BtOrtodongrama_Click"/>
            <Button x:Name="btSalvar" Content="Salvar" Margin="0,0,145,30" VerticalAlignment="Bottom" Height="35" HorizontalAlignment="Right" Width="90" Click="BtSalvar_Click"/>
            <Button x:Name="btVoltar" Content="Voltar" HorizontalAlignment="Right" Margin="0,0,40,30" VerticalAlignment="Bottom" Width="90" Height="35" Click="BtVoltar_Click" />
            <Label x:Name="label" Content="Cliente:" Margin="0,0,927,639" FontSize="30" HorizontalAlignment="Right" VerticalAlignment="Bottom"/>
            <Label x:Name="label1" Content="{Binding Cliente.Nome, Mode=OneWay}" Margin="402,31,346,639" FontSize="30"/>

        </Grid>
    </Viewbox>
</Window>
