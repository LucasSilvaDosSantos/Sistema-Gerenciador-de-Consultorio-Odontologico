        <Label x:Name="label10" Content="UF" HorizontalAlignment="Left" Margin="538,0,0,263" VerticalAlignment="Bottom"/>
        <Label x:Name="label11" Content="Cidade" HorizontalAlignment="Left" Margin="91,0,0,235" VerticalAlignment="Bottom"/>
        <Label x:Name="label12" Content="CEP" HorizontalAlignment="Left" Margin="370,0,0,232" VerticalAlignment="Bottom"/>
        <TextBox x:Name="tbNome" Height="23" Margin="139,214,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" HorizontalAlignment="Left" Width="488"/>
        <TextBox x:Name="tbDataDeNascimento" Height="23" Margin="214,242,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" HorizontalAlignment="Left" Width="413"/>
