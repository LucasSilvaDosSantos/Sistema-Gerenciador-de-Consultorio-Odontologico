        <Rectangle Fill="White" HorizontalAlignment="Right" Margin="0,242,82,235" Stroke="Black" Width="534"/>
        <TextBox x:Name="tbObs" HorizontalAlignment="Right" Margin="0,242,82,235" TextWrapping="Wrap" Text="" Width="534"/>
        <Label x:Name="label5_Copy" Content="Celular 1" HorizontalAlignment="Left" Margin="91,335,0,317"/>
