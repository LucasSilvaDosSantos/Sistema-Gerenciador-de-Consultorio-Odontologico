        <TextBox x:Name="tbCelular1" Margin="152,330,0,325" TextWrapping="Wrap" Text="" HorizontalAlignment="Left" Width="198"/>
        <TextBox x:Name="tbCelular2" Margin="416,330,0,325" TextWrapping="Wrap" Text="" HorizontalAlignment="Left" Width="211"/>
        <TextBox x:Name="tbEndereco" Margin="155,0,0,297" TextWrapping="Wrap" Text="" HorizontalAlignment="Left" Width="472" Height="28" VerticalAlignment="Bottom"/>
